{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "content/_apiSource/*.dll"
                    ]
                }
            ],
            "dest": "content/api",
            "filter": "configuration/filterConfig.yml"
        }
    ],
    "build": {
        "content": [
            {
                "files": [ "content/te3/**/*.md", "content/te2/**/*.md", "content/onboarding/**/*.md", "content/common/**/*.md", "content/*.md", "content/toc.yml", "content/api/*", "content/kb/**/*.md" ]
            }
        ],
        "template": [
            "default",
            "templates/tabulareditor"
        ],
        "resource": [
            {
                "files": [ "assets/images/**", "content/te3/images/**", "content/whats-new/**", "content/404.html", "content/te3/features/images/**" ]
            }
        ],
        "globalMetadata": {
            "_appTitle": "Tabular Editor Documentation",
            "_enableSearch": true,
            "_appFaviconPath": "assets/images/te3logo.png",
            "_enableNewTab": true,
            "_disableNavbar": true
        },
        "markdownEngineName": "markdig",
        "dest": "_site",
        "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
    }
}