    <script>
      /**
       * Dynamically match search bar width to content area
       * Uses getBoundingClientRect for exact rendered dimensions
       */
      (function() {
        function updateSearchWidth() {
          const contentElement = document.querySelector("#main-body > main > div.content");
          
          if (contentElement) {
            // Use getBoundingClientRect for exact rendered width including padding
            const contentWidth = contentElement.getBoundingClientRect().width;
            
            // Set CSS custom property
            document.documentElement.style.setProperty('--search-width', contentWidth + 'px');
          }
        }
        
        // Update on various events
        window.addEventListener('load', updateSearchWidth);
        window.addEventListener('resize', updateSearchWidth);
        
        // Update after short delay to catch dynamic content
        setTimeout(updateSearchWidth, 100);
        setTimeout(updateSearchWidth, 500);
        
        // Update when DOM content is fully loaded
        if (document.readyState === 'loading') {
          document.addEventListener('DOMContentLoaded', updateSearchWidth);
        } else {
          updateSearchWidth();
        }
      })();
    </script>
